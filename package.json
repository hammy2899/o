{
  "name": "o",
  "version": "1.0.0",
  "description": "common object helper functions",
  "main": "dist/o.js",
  "scripts": {
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "clean": "rm -rf ./dist",
    "build:rollup": "rollup -c",
    "build:watch": "rollup -c -w",
    "build:uglify": "npx uglifyjs dist/o.js -o dist/o.min.js",
    "build": "npm-run-all clean build:rollup build:uglify",
    "test:jest": "jest --coverage --verbose --runInBand --no-cache",
    "test:watch": "jest --coverage --verbose --runInBand --no-cache --watch",
    "test:eslint": "eslint src",
    "test": "npm-run-all test:eslint test:jest",
    "docs:clean": "rm -rf docs",
    "docs:esdoc": "esdoc -c .esdoc.json",
    "docs:cname": "echo o.hammy2899.xyz >> docs/CNAME",
    "docs": "npm-run-all docs:clean docs:esdoc docs:cname",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hammy2899/o.git"
  },
  "keywords": [
    "object",
    "helper"
  ],
  "author": "hammy2899",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hammy2899/o/issues"
  },
  "homepage": "https://github.com/hammy2899/o#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "coveralls": "^3.0.2",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.5",
    "regenerator-runtime": "^0.13.1",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "standard-version": "^4.4.0",
    "uglify-js": "^3.4.9"
  },
  "dependencies": {
    "circle-assign": "^2.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*.js"
    ]
  }
}
